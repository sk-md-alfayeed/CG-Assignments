<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context" 
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
http://www.springframework.org/schema/context 
http://www.springframework.org/schema/context/spring-context-3.0.xsd" >

 <context:annotation-config />
<bean name="book1" class="com.cg.springcore.a8a9a10.Book1" >
	<property name="bookName" value="The Book"/>
	<property name="bookPrice" value="500"/>
</bean>
<bean name="book2" class="com.cg.springcore.a8a9a10.Book2" init-method="init" destroy-method="destroy">
	<property name="bookName" value="The UnBook"/>
	<property name="bookPrice" value="199"/>
</bean>
<bean name="book3" class="com.cg.springcore.a8a9a10.Book3">
	<property name="bookName" value="The Bookless"/>
	<property name="bookPrice" value="150"/>
</bean>

</beans>