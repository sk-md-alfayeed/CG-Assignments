<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.0.xsd" > 

<context:annotation-config />
<bean name="Book" class="com.cg.springcore.a5.Book">
<property name="bookName" value="The Book"></property>
<property name="price" value="500"></property>
</bean>
<bean name="author" class="com.cg.springcore.a5.Author">
<property name="authorName" value="Book Man"></property>
<property name="authorId" value="007"></property>
</bean>
<bean name="publisher" class="com.cg.springcore.a5.Publisher">
 <property name="publisherName" value="Book Publishers"></property>
<property name="publisherId" value="3000"></property>
</bean>

</beans>